@page
@model Prismproject_A.Views.Home.UserModel
@{
}

<h2 class="text-center">Welcome to your account</h2>

<div>
    First Name: @Model.appUser?.FirstName
</div>
<div>
    Last Name: @Model.appUser?.LastName
</div>
<div>
    Email: @Model.appUser?.Email
</div>
<div>
    District: @Model.appUser?.District
</div>
<div>
    Created At: @Model.appUser?.CreatedAt.ToString("MM/dd/yyyy")
</div>
